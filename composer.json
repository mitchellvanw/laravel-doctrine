{
    "name": "mitchellvanw/laravel-doctrine",
    "description": "The Doctrine 2 implementation that melts with Laravel",
    "keywords": ["doctrine", "doctrine 2", "orm", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitchell van Wijngaarden",
            "email": "mitchell@kooding.nl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "doctrine/orm": "2.5.*",
        "doctrine/migrations": "1.*"
    },
	"require-dev": {
		"mockery/mockery": "dev-master",
		"phpunit/phpunit": "3.7.*"
	},
    "autoload": {
        "psr-4": {
            "Mitch\\LaravelDoctrine\\": "src/",
	        "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
