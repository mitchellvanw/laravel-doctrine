{
    "name": "mitchellvanw/laravel-doctrine",
    "description": "The Doctrine 2 implementation that melts with Laravel 4",
    "version": "0.5.0",
    "keywords": ["doctrine", "doctrine 2", "orm", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitchell van Wijngaarden",
            "email": "mitchell@kooding.nl"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "4.*|5.*",
        "doctrine/orm": "2.5.*",
        "doctrine/migrations": "1.*"
    },
	"require-dev": {
        "illuminate/container": "4.*|5.*",
        "illuminate/console": "4.*|5.*",
		"mockery/mockery": "dev-master",
		"phpunit/phpunit": "3.7.*"
	},
    "autoload": {
        "psr-4": {
            "Mitch\\LaravelDoctrine\\": "src/",
	        "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
